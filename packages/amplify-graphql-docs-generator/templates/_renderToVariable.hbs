{{#each refFragments }}
  import { {{camelCase name}}Fragment } from "./fragments"
{{/each}}

{{#each fragments }}
  export const {{camelCase name}}Fragment =  /* GraphQL */`{{#format }}
      {{> renderExternalFragment }}
    {{/format}}
    {{> renderRefFragments }}`;

{{/each}}

{{#each operations }}
  export const {{camelCase name}} =  /* GraphQL */`{{#format }}
      {{> renderOp }}
    {{/format}}
    {{> renderRefFragments }}`;

{{/each}}